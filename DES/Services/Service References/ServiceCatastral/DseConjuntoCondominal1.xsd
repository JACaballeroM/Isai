<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="http://tempuri.org/DseConjuntoCondominal.xsd" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:mstns="http://tempuri.org/DseConjuntoCondominal.xsd" attributeFormDefault="qualified" elementFormDefault="qualified" targetNamespace="http://tempuri.org/DseConjuntoCondominal.xsd" id="DseConjuntoCondominal" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element msdata:IsDataSet="true" msdata:UseCurrentLocale="true" name="DseConjuntoCondominal">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="ConjuntoCondominal">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="IDPREDIO" type="xs:decimal" />
              <xs:element msdata:Caption="Región" name="REGION">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="3" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element msdata:Caption="Manzana" name="MANZANA">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="3" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element msdata:Caption="Lote" name="LOTE">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="2" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element msdata:Caption="Tipo condominio" name="DESCRIPCIONTIPOSCONDO">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element msdata:Caption="Descripción" name="DESCRIPCIONGRUPO">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="COLONIA">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="ABREVIATURA">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="8" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element msdata:Caption="Delegación" name="DELEGACION">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="25" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element msdata:Caption="Superficie" minOccurs="0" name="SUPERFICIE" type="xs:decimal" />
              <xs:element name="IDGRUPOCONDOMINAL" type="xs:decimal" />
              <xs:element msdata:ReadOnly="true" msdata:Expression="COLONIA + IIF(ISNULL(ABREVIATURA,'') = '', '', ' (' + TRIM(ABREVIATURA) + ')')" msdata:Caption="Colonia" minOccurs="0" name="ColoniaDesc" type="xs:string" />
              <xs:element minOccurs="0" name="REPGRAFICA">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="1" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>