  <xs:annotation>
    <xs:appinfo>
      <msdata:Relationship name="Via_Colonias" msdata:parent="Via" msdata:child="Colonias" msdata:parentkey="IDVIA" msdata:childkey="IDVIA" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" />
