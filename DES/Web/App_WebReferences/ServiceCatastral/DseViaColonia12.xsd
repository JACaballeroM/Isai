<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="http://tempuri.org/DseViaColonia.xsd" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:mstns="http://tempuri.org/DseViaColonia.xsd" attributeFormDefault="qualified" elementFormDefault="qualified" targetNamespace="http://tempuri.org/DseViaColonia.xsd" id="DseViaColonia" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element msdata:IsDataSet="true" msdata:UseCurrentLocale="true" name="DseViaColonia">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="Colonias">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="IDCOLONIA" type="xs:decimal" />
              <xs:element name="COLONIA">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="IDDELEGACION" type="xs:decimal" />
              <xs:element name="DELEGACION">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="25" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="ABREVIATURA">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
